@article{Neimark2003a,
	Abstract = {With the example of the capillary condensation of Lennard-Jones fluid in nanopores ranging from 1 to 10 nm, we show that the non-local density functional theory (NLDFT) with properly chosen parameters of intermolecular interactions bridges the scale gap from molecular simulations to macroscopic thermodynamics. On the one hand, NLDFT correctly approximates the results of Monte Carlo simulations (shift of vapour--liquid equilibrium, spinodals, density profiles, adsorption isotherms) for pores wider than about 2 nm. On the other hand, NLDFT smoothly merges (above 7--10 nm) with the Derjaguin--Broekhoff--de Boer equations which represent augmented Laplace--Kelvin equations of capillary condensation and desorption.},
	Author = {Alexander V Neimark and Peter I Ravikovitch and Aleksey Vishnyakov},
	Date-Added = {2017-08-30 10:27:37 +0000},
	Date-Modified = {2017-08-30 10:27:37 +0000},
	Journal = {Journal of Physics: Condensed Matter},
	Number = {3},
	Pages = {347},
	Title = {Bridging scales from molecular simulations to classical thermodynamics: density functional theory of capillary condensation in nanopores},
	Url = {http://stacks.iop.org/0953-8984/15/i=3/a=303},
	Volume = {15},
	Year = {2003},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://stacks.iop.org/0953-8984/15/i=3/a=303}}

