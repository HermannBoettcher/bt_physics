	Month = {{NOV 1}},
	Number = {{43}},
	Pages = {{15736-15742}},
	Title = {{Microstructural characterization of adsorption and depletion regimes of supercritical fluids in nanopores}},
	Unique-Id = {{ISI:000250559900030}},
	Volume = {{111}},
	Year = {{2007}},
	Bdsk-Url-1 = {http://dx.doi.org/10.1021/jp073698c%7D}}

