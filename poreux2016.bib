	Pages = {2059-2077},
	Researcherid-Numbers = {Chen, Hongyin/J-2955-2013},
	Title = {Capillary condensation in a disordered mesoporous medium: a grand canonical Monte Carlo study},
	Unique-Id = {ISI:000176758600007},
