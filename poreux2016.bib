	Issn = {292-8941},
	Journal = {Eur. Phys. J. E},
	Month = {JUL},
	Number = {4},
	Pages = {437-443},
	Title = {Roughness and dynamics of a contact line of a viscous fluid on a disordered substrate},
	Unique-Id = {ISI:000178451200010},
	Url = {http://dx.doi.org/10.1140/epje/i2002-10032-2%7D},
