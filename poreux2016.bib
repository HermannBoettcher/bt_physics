	Month = {{JUL 1}},
	Number = {{13}},
	Pages = {{6429-6432}},
	Title = {{Probing memory effects in confined fluids via diffusion measurements}},
	Unique-Id = {{ISI:000257101100009}},
	Volume = {{24}},
	Year = {{2008}},
	Bdsk-Url-1 = {http://dx.doi.org/10.1021/la801349y%7D}}

