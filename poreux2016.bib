@Article{Evans1986b,
  author     = {Evans, Robert and Marconi, Umberto Marini Bettolo and Tarazona, Pedro},
  title      = {Capillary condensation and adsorption in cylindrical and slit-like pores},
  journal    = {J. Chem. Soc.{,} Faraday Trans. 2},
  year       = {1986},
  volume     = {82},
  pages      = {1763-1787},
  abstract   = {The nature of adsorption of simple fluids confined in model pores is investigated by means of a density functional approach. For temperatures corresponding to a partial wetting situation a first-order phase transition (capillary condensation) from dilute {'}gas{'} to dense {'}liquid{'} occurs at relative pressures / close to those predicted by the macroscopic Kelvin equation{,} even for radii or wall separations as small as 10 molecular diameters. In a complete wetting situation{,} where thick films develop{,} the Kelvin equation is{,} in general{,} not accurate. At fixed the adsorption [Gamma]() exhibits a loop; [Gamma] jumps discontinuously at the first-order transition{,} but the accompanying metastable portions of the loop could produce hysteresis similar to that observed in adsorption measurements on mesoporous solids. Metastable thick films persist to larger / in slits than in cylinders and this has repercussions for the shape of hysteresis loops. For a given pore size the loop in [Gamma] shrinks with increasing and disappears at a capillary critical temperature (< ). If > condensation no longer occurs and hysteresis of [Gamma] will not be observed. Such behaviour is found in experiments. A prewetting (thick-thin film) transition can occur for confined fluids. The transition is shifted to a smaller value of / than that appropriate to prewetting at a single planar wall. Whereas the magnitude of the shift is very small for slits{,} it is substantial for cylinders and this leads to the possibility of finding a triple point{,} where {'}liquid{'} and thick and thin films coexist{,} in cylindrical pores whose radii may not be too large for investigation by experiment or computer simulation. Adsorption of supercritical fluids ( > {,} the bulk critical temperature) in cylinders is mentioned briefly.},
  bdsk-url-1 = {http://dx.doi.org/10.1039/F29868201763},
  doi        = {10.1039/F29868201763},
  file       = {:Evans1986b.pdf:PDF},
  issue      = {10},
  publisher  = {The Royal Society of Chemistry},
  url        = {http://dx.doi.org/10.1039/F29868201763},
}

